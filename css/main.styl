/* Estilo de Tramites/flujos*/
.flujoTramites
    background-color color_gris_claro
    //margin margen_content
    div
        margin 0 auto
        float left
        position relative
        width 95%
        background-color color_gris_claro
    .detalle
        margin 0 auto
        font-family Allerta
        padding 1em
        div
            text-align left
            
        .fila
            margin 0 auto
            width 100%
            .etiqueta
                font-size 1em
                color color_gris_medio
                background-color color_gris_claro
                padding 0.25em 0 0.25em 0
            .informacion
                background-color rgba(255,255,255,100%)
                border solid 0.15em color_gris_claro_m
                color color_fondo_rojo
                font-size 1em
                height 1.25em
                padding 0.25em 0 0.25em 0.25em
    
    .flujo
        margin 0 auto
        padding 1em
        position relative
        text-align center
        font-family Allerta
        div
            //border-radius radio_lista
            border-radius 0.2em
        .barraIndicadorV,.barraIndicadorA,.barraIndicadorG
            z-index 1
            box-shadow 0.2em 0 rgba(0,0,0,10%)
            position relative
            top 1.5em
            left -92.9%
            width 0.9em
            height 4.2em
        .barraIndicadorV
            background-color rgba(79,163,51,100%)
        .barraIndicadorA
            background-color rgba(221,179,51,100%)
        .barraIndicadorG
            background-color rgba(165,165,165,100%)
        .flujoAprobacion,.flujoAprueba
            border solid 0.1em color_gris_claro_m
            background-color white
            
            span
                position relative
                left 0
                right 0
            .indicador
                z-index 2
                position absolute
                background-color rgba(50,50,50,100%)
        .flujoAprobacion
            //font-size tamano_fuente_variable
            border-radius 0.25em
            font-size 1.2em
            height 2em
            margin 0.5em 0 0.5em 0
            span
                top 10%
                bottom 10%
            .indicador
                left 0.3em
                top 0.3em
                width 0.8em
                height 0.8em
                border-radius 2em
                padding 0.3em
                .icon-check
                    background-color rgba(0,0,0,100%)
                    position absolute
                    border-radius 2em
                    left 0.2em
                    top 0.2em
                    width 0.6em
                    height 0.6em
                    padding 0.2em
        .flujoAprueba
            border solid 0.2em color_gris_claro_m
            border-radius 0.5em
            color color_gris_medio
            font-size 0.8em
            font-weight bolder
            height 4em
            margin 0.45em 0 0.45em 0
            span
                font-size 1.1em
                top 30%
                bottom 30%
            .indicador
                left 0.6em
                top 0.5em
                width 1.6em
                height 1.6em
                border-radius 2em
                padding 0.20em
                .icon-check
                    background-color rgba(255,255,255,100%)
                    border-radius 2em
                    color rgba(79,163,51,100%)
                    height 1em
                    left 0.3em
                    padding 0.2em
                    position absolute
                    top 0.3em
                    width 1em
