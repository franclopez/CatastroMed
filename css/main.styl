body
    background: color_fondo_main

    header
        overflow: hidden

        .header-cabecera-verde
            background: color_fondo_verde
            border: 1px solid color_fondo_verde 
            height: 50px
            position: relative
            vertical-align: middle

            .atras
                position: absolute
                top: 0.5em
                left: 10px
                vertical-align: middle
                .icon-flecha-izq
                    color: white
                    font-size: 2em
                    text-decoration: none
                    vertical-align: middle
                    &:hover 
                        color color_fondo_rojo2
                        text-shadow: 1px 1px 10px rgba(black, 0.5)

            .titulo 
                .header-titulo
                    color: white
                    font-family: open_sansregular
                    font-size: 20px
                    font-weight: bold
                    text-align: center
                    vertical-align: middle

            .botonera
                position: absolute
                top: 0.5em
                right: 10px
                vertical-align: middle

                #btn-cerrar
                    color: white
                    font-size: 2.3em
                    position: relative
                    text-decoration: none
                    vertical-align: middle

                    &:hover
                        color color_fondo_rojo2
                        text-shadow: 1px 1px 10px rgba(black, 0.5)
            .regresar
                position: absolute
                top: 0.5em
                left: 10px
                vertical-align: middle

                #btn-regresar
                    color: white
                    font-size: 2.3em
                    position: relative
                    text-decoration: none
                    vertical-align: middle

                    &:hover
                        color color_fondo_rojo2
                        text-shadow: 1px 1px 10px rgba(black, 0.5)
    
    .header-cabecera-roja
        background: gray
        display: table
        margin: 10 0 10 0

        ul
            display: table-row
            margin: 0
            list-style: none
            overflow-x auto
            padding: 0
            white-space: nowrap
            width: 100%

            li                 
            .primero
                background: color_fondo_rojo
                color: white
                display: table-cell;
                width: 100px

                .icon-inicio
                    color: white
                    font-size: 30px
                    text-decoration: none
                    vertical-align: middle

                .icon.icon-info
                    color: white
                    font-size: 30px
                    text-decoration: none
                    vertical-align: middle

            .segundo
                background: color_fondo_rojo 
                display: table-cell;
                position: relative
                text-align: center
                width: 100%   

                a 
                    color white
                    font-size: 1.5em  
                    position: relative
                    text-decoration: none 

                    &:hover
                        background: rgba(black, 0.1)
                        box-shadow: 2px 2px 10px rgba(black, 0.2)


                    .triangulo
                        position: absolute
                        right: 0px
                        bottom:  -1em
                        font-family: iconosCatastro
                        font-size: 15px
                     
                        &:after                  
                            content: "\e624"



            .tercero
                background: color_fondo_rojo2   
                display: table-cell;  
                padding: 0.7em 1em
                width: 100px

                .icon-compartir
                    font-size: 30px
                    color: white
                    text-decoration: none
                    vertical-align: middle

                    &:hover
                        color color_fondo_verde
                        text-shadow: 1px 1px 10px rgba(black, 0.7)

